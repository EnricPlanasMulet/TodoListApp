{
  "expo": {
    "name": "ToDo List",
    "slug": "ToDoList",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#000000ff",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
      
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.muletdev.todolist",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.muletdev.todolist",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0d4ae3ff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
